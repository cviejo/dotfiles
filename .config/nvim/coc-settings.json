{
	"coc.preferences.formatOnSaveFiletypes": ["svelte", "javascript", "rust"],
	"diagnostic.errorSign": "✖",
	"diagnostic.hintSign": "ẍ",
	"diagnostic.infoSign": "⚐",
	"diagnostic.warningSign": "⚠",
	"eslint.autoFixOnSave": true,
	"eslint.filetypes": ["javascript", "svelte", "javascriptreact", "typescript", "typescriptreact"],
	"explorer.buffer.showHiddenBuffers": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.previewAction.onHover": false,
	"javascript.suggestionActions.enabled": false,
	"prettier.onlyUseLocalVersion": true,
	"prettier.printWidth": 100,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"snippets.priority": 100,
	"suggest.noselect": false,
	"svelte.plugin.typescript.enable": false
}
